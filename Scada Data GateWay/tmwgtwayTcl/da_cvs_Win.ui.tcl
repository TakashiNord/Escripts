  grid $base.label#4 -in $root  -row 5 -column 3  \
    -sticky nesw
  grid $base.listbox -in $root  -row 5 -column 4  \
    -sticky nesw
  grid $base.button#2 -in $root -row 9 -column 4  \
    -sticky nesw
  grid $base.label_A -in $root  -row 10 -column 4  \
    -sticky new
  grid $base.scroll2 -in $root  -row 11 -column 1  \
    -sticky new

  # Resize behavior management

  grid rowconfigure $root 1 -weight 0 -minsize 16
  grid rowconfigure $root 2 -weight 0 -minsize 12
  grid rowconfigure $root 3 -weight 0 -minsize 8
  grid rowconfigure $root 4 -weight 0 -minsize 9
  grid rowconfigure $root 5 -weight 0 -minsize 9
