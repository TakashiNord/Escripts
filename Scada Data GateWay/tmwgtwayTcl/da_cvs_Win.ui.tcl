proc daRun { } {
global tcl_platform
#
global entryTNS
global entryLogin
global entryPswd
global entryList

#
set f "da_dev.tcl"
set paths [info script]
set paths [ file normalize $paths ]
set dirname [ file dirname $paths ]
set f0 [file join $dirname $f ]
set f [ file nativename $f0 ]
#
source $f

set tns $entryTNS
set usr $entryLogin
set pwd $entryPswd
set crn [.listbox curselection ]
if {$crn==""} { set crn 1 ;}


  catch { .text delete 1.0 end ;  }

  set t1 [ clock format [ clock seconds ] -format "%T" ]
  set s1 "start = $t1"
  .text insert 0.0 "\n--------------------------------------\n"
  .text insert end "\nstart = $t1\n"

  set ls {};
  tmwgtway $tns $usr $pwd $crn ls

  for {set ind 0} {$ind<[llength $ls]} {incr ind} {
      .text insert end "[lindex $ls $ind ]\n" ;
  }

  set t1 [ clock format [ clock seconds ] -format "%T" ]
  .text insert end "\nend = $t1\n"

 .text mark set insert 0.0

 update;

 return 0 ;
}
































