proc da_cvs_Win_ui {root args} {

  # this treats "." as a special case

  if {$root == "."} {
      set base ""
  } else {
      set base $root
  }

  text $base.text \
    -cursor {} \
    -height 1 \
    -width 1 \
    -xscrollcommand {list .scroll2 set} \
    -yscrollcommand {list .scroll set}
  catch {
    $base.text configure \
      -font {-*-MS Sans Serif-Medium-R-Normal-*-*-70-*-*-*-*-*-*}
  }

  scrollbar $base.scroll \
    -command {list .text yview} \
    -orient v

  label $base.label#1 \
    -justify right \
    -text TNS:
  catch {
    $base.label#1 configure \
      -font {-*-MS Sans Serif-Bold-R-Normal-*-*-80-*-*-*-*-*-*}
  }

  entry $base.entry#1 \
    -background yellow \
    -textvariable entryTNS

  label $base.label#2 \
    -justify right \
    -text Login:
  catch {
    $base.label#2 configure \
      -font {-*-MS Sans Serif-Bold-R-Normal-*-*-80-*-*-*-*-*-*}
  }

  entry $base.entry#2 \
    -background yellow \
    -textvariable entryLogin

  label $base.label#3 \
    -justify right \
    -text pass:
  catch {
    $base.label#3 configure \
      -font {-*-MS Sans Serif-Bold-R-Normal-*-*-80-*-*-*-*-*-*}
  }

  entry $base.entry#3 \
    -background yellow \
    -textvariable entryPswd

  label $base.label#4 \
    -justify right \
    -text Names:

  listbox $base.listbox \
    -height 0 \
    -width 0

  button $base.button#2 \
    -command daRun \
    -text Run

  label $base.label_A

  scrollbar $base.scroll2 \
    -orient h


  # Geometry management

  grid $base.text -in $root -row 1 -column 1  \
    -rowspan 10 \
    -sticky nesw
  grid $base.scroll -in $root -row 1 -column 2  \
    -rowspan 10 \
    -sticky nsw
  grid $base.label#1 -in $root  -row 1 -column 3  \
    -sticky nesw
  grid $base.entry#1 -in $root  -row 1 -column 4  \
    -sticky nesw
  grid $base.label#2 -in $root  -row 2 -column 3  \
